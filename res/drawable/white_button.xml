<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_pressed="true" >
	    <shape android:shape="rectangle">
		  <gradient  android:angle="-90" android:startColor="#a6a89f" android:endColor="#d5d6d1" />
		  <stroke android:width="1dp" android:color="#b7b9ac" />
	 	  <corners android:radius="5dp" />
	    </shape>
	</item>
	<item>
		<shape android:shape="rectangle">
		  <gradient  android:angle="-90" android:startColor="#fefefe" android:endColor="#d5d6d1" /> 
		  <stroke android:width="1dp" android:color="#b7b9ac" />
		  <corners android:radius="5dp" />
	  </shape>
  	</item>
</selector>
